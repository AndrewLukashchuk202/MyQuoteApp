<div id="<%= dom_id quote %>">
  <p>
    <strong>Text:</strong>
    <%= quote.text %>
  </p>

  <p>
    <strong>Publication year:</strong>
    <%= quote.publication_year %>
  </p>

  <p>
    <strong>Is public:</strong>
    <%= quote.is_public %>
  </p>

  <p>
    <strong>User comment:</strong>
    <%= quote.user_comment %>
  </p>

  <p>
    <strong>User:</strong>
    <%= quote.user_id %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= quote.author_id %>
  </p>

</div>
